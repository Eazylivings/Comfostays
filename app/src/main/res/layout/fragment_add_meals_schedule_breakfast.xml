<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.comfostays.activities.buildingsetup.AddMealsScheduleActivity$PlaceholderFragment">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Time"
        android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_timeHeading"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragmentAddMealsScheduleBreakfast_textView_timeHeading"
        android:layout_alignParentStart="true"
        android:id="@+id/fragmentAddMealsScheduleBreakfast_gridLayout_time">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="From :"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_fromTime"
            android:layout_row="0"
            android:layout_column="0"
            android:gravity="center" />

        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:inputType="time"
            android:ems="10"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_fromTime"
            android:layout_row="0"
            android:layout_column="1" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="To :"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_toTime"
            android:layout_row="0"
            android:layout_column="2"
            android:layout_marginLeft="10dp"
            android:gravity="center" />

        <EditText
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:inputType="time"
            android:ems="10"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_toTime"
            android:layout_row="0"
            android:layout_column="3" />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/fragmentAddMealsScheduleBreakfast_gridLayout_time"
        android:layout_alignParentStart="true"
        android:id="@+id/fragmentAddMealsScheduleBreakfast_gridLayout_schedule">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Monday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_monday"
            android:layout_row="0"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_monday"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Tuesday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_tuesday"
            android:layout_row="1"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_tuesday"
            android:layout_row="1"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Wednesday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_wednesday"
            android:layout_row="2"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_wednesday"
            android:layout_row="2"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Thursday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_thursday"
            android:layout_row="3"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_thursday"
            android:layout_row="3"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Friday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_friday"
            android:layout_row="4"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_friday"
            android:layout_row="4"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Saturday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_saturday"
            android:layout_row="5"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_saturday"
            android:layout_row="5"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Sunday"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_textView_sunday"
            android:layout_row="6"
            android:layout_column="0" />

        <EditText
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:id="@+id/fragmentAddMealsScheduleBreakfast_editText_sunday"
            android:layout_row="6"
            android:layout_column="1"
            android:layout_marginLeft="20dp"
            android:background="@drawable/rounded_edittext"
            android:layout_marginBottom="5dp" />
    </GridLayout>

</RelativeLayout>
